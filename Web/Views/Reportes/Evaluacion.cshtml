
<div class="col-md-6">
    <canvas id="grafico6"></canvas>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryajax")
    <!--Librería para gráficos-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.2.2/Chart.bundle.min.js"></script>
    <script src="~/Scripts/graficoChartJS.js"></script>
    <script type="text/javascript">

        var colores = @Html.Raw(Json.Encode(ViewBag.grafico6.Colores));
        colores = colores.split(",");
        var etiquetas = @Html.Raw(Json.Encode(ViewBag.grafico6.Etiquetas));
        etiquetas = etiquetas.split(",");
        var valores = @Html.Raw(Json.Encode(ViewBag.grafico6.Valores));
        valores = valores.split(",");
        var titulo = @Html.Raw(Json.Encode(ViewBag.grafico6.titulo));
        var tituloEtiquetas = @Html.Raw(Json.Encode(ViewBag.grafico6.tituloEtiquetas));
        var tipo = @Html.Raw(Json.Encode(ViewBag.grafico6.tipo));
        grafico6(titulo, tituloEtiquetas, tipo, colores, etiquetas, valores);


    </script>
}



@{
    ViewBag.Title = "Evaluacion";
}

<h2>Evaluacion</h2>

